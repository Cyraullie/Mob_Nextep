{
  "expo": {
    "name": "Nextep",
    "slug": "Nextep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_splash.png",
      "backgroundColor": "#3e2bc5",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon":  "./assets/logo.png",
      "splash": {
        "image": "./assets/logo_splash.png",
        "backgroundColor": "#3e2bc5",
        "resizeMode": "contain"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/logo_splash.png",
        "backgroundColor": "#3e2bc5",
        "resizeMode": "contain"
      },
      "package": "com.cyril.Nextep"
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
